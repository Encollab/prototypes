<mvc:View controllerName="encollab.dp.useradmin.controller.Users" xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:semantic="sap.m.semantic">
    <semantic:MasterPage id="page" title="{i18n>usersViewTitle}" navButtonPress="onNavBack" showNavButton="true">
        <semantic:content>
            <List id="list" items="{
						path: 'users'
					}" busyIndicatorDelay="0" noDataText="{masterView>/noDataText}" mode="{= ${device>/system/phone} ? 'None' : 'SingleSelectMaster'}" growing="true" growingScrollToLoad="true" selectionChange="onSelectionChange">
                <items>
                    <ObjectListItem type="{= ${device>/system/phone} ? 'Active' : 'Inactive'}" press="onSelectionChange" title="{name}">
                    </ObjectListItem>
                </items>
            </List>
        </semantic:content>
    </semantic:MasterPage>
</mvc:View>